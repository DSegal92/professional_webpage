/****** RESET ******/

body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,textarea,p,blockquote,th,td { 
	margin:0;
	padding:0;
}
table {
	border-collapse:collapse;
	border-spacing:0;
}
fieldset,img { 
	border:0;
}
address,caption,cite,code,dfn,em,strong,th,var {
	font-style:normal;
	font-weight:normal;
}
ol,ul {
	list-style:none;
}
caption,th {
	text-align:left;
}
h1,h2,h3,h4,h5,h6 {
	font-size:100%;
	font-weight:normal;
}
q:before,q:after {
	content:'';
} 
abbr,acronym { border:0;
}

/***** FONTS *****/
@font-face {
    font-family: 'edmondsans_medium';
    src: url('<%= asset_path 'edmondsans-medium-webfont.eot'%>');
    src: url('<%= asset_path 'edmondsans-medium-webfont.eot?#iefix'%>') format('embedded-opentype'),
         url('<%= asset_path '/assets/edmondsans-medium-webfont.woff'%>') format('woff'),
         url('<%= asset_path '/assets/edmondsans-medium-webfont.ttf'%>') format('truetype'),
         url('<%= asset_path '/assets/edmondsans-medium-webfont.svg#edmondsansmedium'%>') format('svg');
